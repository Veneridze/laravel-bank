{
    "name": "veneridze/laravel-bank",
    "keywords": [
        "laravel",
        "bank",
        "ipg",
        "payment",
        "gateway",
        "payment gateway",
        "internet payment gateway"
    ],
    "type": "library",
    "description": "A package to connect to any IPG to make a payment",
    "license": "MIT",
    "authors": [
        {
            "name": "Farshad Haassani",
            "email": "farshad.haassani73@gmail.com"
        },
        {
            "name": "Veneridze",
            "email": "viacheslav.kopaev@yandex.ru"
        }
    ],
    "require": {
        "ext-soap": "*",
        "ext-curl": "*",
        "laravel/framework": "^5.6|^6|^7|^8|^9|^10|^11",
        "hekmatinasser/verta": "^2.1",
        "movemoveapp/laravel-dadata": "^1.0",
        "spatie/laravel-package-tools": "^1.18"
    },
    "autoload": {
        "psr-4": {
            "Farsh4d\\Bank\\": "src/"
        }
    },
    "extra": {
